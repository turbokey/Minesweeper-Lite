Sub Find_clean_place
  Dim i As Int, j As Int
          For i = 1 To SizeY
            For j = 1 To SizeX
             If Doska(i, j) <> -1 Then
               Doska(i, j) = -1
               Return
            End If
          Next
        Next
End Sub
Sub Clear_sosedi
  Dim i As Int, j As Int
          For i = 0 To SizeY + 1
            For j = 0 To SizeX
             If Doska(i, j) <> -1 Then
               Doska(i, j) = 0
            End If
          Next
        Next
End Sub
Sub Count_sosedi
  Dim i As Int, j As Int, i1 As Int, j1 As Int
          For i = 1 To SizeY
            For j = 1 To SizeX
             If Doska(i, j) = -1 Then
               For i1 = i - 1 To i + 1
                 For j1 = j - 1 To j + 1
                  If Doska(i1, j1) >= 0 Then
                    Doska(i1, j1) = Doska(i1, j1) + 1
                  End If
                Next
              Next
            End If
          Next
        Next
                    
End Sub
